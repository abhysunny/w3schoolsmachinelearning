    "#Progrm to find mean, median and mod\n",
    "\n",
    "import numpy\n",
    "from scipy import stats\n",
    "\n",
    "\n",
    "marks = [45,60,63,60,73,67,88,99]\n",
    "\n",
    "x = numpy.mean(marks)\n",
    "y = numpy.median(marks)\n",
    "z = stats.mode(marks)\n",
    "\n",
    "print (\"mean value is \" + str(x))\n",
    "print (\"median value is \" + str(y))\n",
    "print (\"mode value is \" + str(z))\n"
   ]
